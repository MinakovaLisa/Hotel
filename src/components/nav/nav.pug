-
  const NAV_ITEM = [
  {ITEM_NAME:"home"},
  {ITEM_NAME:"about us"},
  {ITEM_NAME:"services"},
  {ITEM_NAME:"careers"},
  {ITEM_NAME:"news"},
  {ITEM_NAME:"documentation"}] 
mixin nav()  
  nav(class="nav")
    - for (var i = 0; i < NAV_ITEM.length; i++)
      div(class="nav__item")
        a(class="nav__link") #{NAV_ITEM[i].ITEM_NAME}