-
  const MENU_ITEM = [
  {ITEM_NAME:"home"},
  {ITEM_NAME:"about us"},
  {ITEM_NAME:"services"},
  {ITEM_NAME:"careers"},
  {ITEM_NAME:"news"},
  {ITEM_NAME:"documentation"}
   ] 

mixin menu(options)
  -
    var menuClassName = "menu"
    menuClassName += options.isLogged ? " menu_logged" : ""
    var logged=options.isLogged
  div(class = menuClassName)  
    nav(class="menu__nav")
      - for (var i = 0; i < MENU_ITEM.length; i++)
        div(class="menu__item")
          a(class="menu__link") #{MENU_ITEM[i].ITEM_NAME}
    if !logged
      p hello There
    else
      p hello NN